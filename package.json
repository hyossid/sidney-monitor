{
  "name": "alex-monitor",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "gen:hasura": "graphql-codegen --config src/config/codegen.yaml",
    "gen:contracts": "tsx src/cli/alex-codegen.ts",
    "lint": "eslint ./src --ext ts,tsx",
    "build": "rimraf dist && tsetse",
    "test": "jest --passWithNoTests",
    "ci": "concurrently -r 'yarn lint' 'yarn test'",
    "start": "tsx src/main.ts",
    "hasura:console": "./bin/hasura-cli-console",
    "hasura": "cd hasura-schema && ../bin/hasura-cli"
  },
  "lint-staged": {
    "*.{js,json,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "tabWidth": 2,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "printWidth": 80
  },
  "dependencies": {
    "@google-cloud/logging": "^10.3.3",
    "@stacks/network": "^6.1.0",
    "@stacks/transactions": "^6.1.0",
    "axios": "^1.2.2",
    "clarity-codegen": "^0.2.0",
    "date-fns": "^2.29.3",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "memoizee": "^0.4.15",
    "safe-json-stringify": "^1.2.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "yargs": "^17.6.2",
    "yqueue": "^1.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.3",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@types/coingecko-api": "^1.0.10",
    "@types/jest": "^29.2.5",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3.0.2",
    "@types/safe-json-stringify": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.16",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.2",
    "prettier-plugin-organize-imports": "^3.2.1",
    "rimraf": "^4.0.4",
    "ts-jest": "^29.0.3",
    "tsetse": "^0.0.4",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.3.1",
  "description": "monitors data from alex hasura database",
  "main": "src/main.ts",
  "repository": "https://github.com/alexgo-io/alex-monitor.git",
  "author": "hyossid <huvsid@gmail.com>",
  "license": "MIT"
}
